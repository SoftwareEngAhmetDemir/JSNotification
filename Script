var notification = new Notification('To do list', { body: 'hi lan ulan', icon: 'https://upload.wikimedia.org/wikipedia/commons/thumb/b/b9/Football_iu_1996.jpg/1200px-Football_iu_1996.jpg' });


notification.onclick = function(event) {
    event.preventDefault(); // prevent the browser from focusing the Notification's tab
    window.open("https://google.com/",'_blank');
  }
  
  
